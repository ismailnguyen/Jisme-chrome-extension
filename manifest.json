{
  "manifest_version": 2,

  "name": "Jisme extension",
  "description": "Quickly get credentials of page you are currently browsing",
  "version": "1.0",

  "browser_action": {
  	"default_icon": "icon.png",
	"default_popup": "index.html",
	"default_title": "Jisme extension"
  },
  
  "content_scripts" : [
    {
	"matches": ["https://*/*", "http://*/*"],
        "js": ["autocomplete.js"]
    }
  ],
    
  "permissions": [
	"activeTab",
	"tabs",
   	"http://*/*",
	"https://*/*"
  ]
}
